<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.git_trailblazer.v1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name="com.git_trailblazer.v1.CommentActivity"></activity>
        <activity android:name="com.git_trailblazer.v1.DrawerActivity" />
        <activity android:name="com.git_trailblazer.v1.QuestionnaireActivity" />
        <activity android:name="com.git_trailblazer.v1.RegisterActivity" />
        <activity android:name="com.git_trailblazer.v1.LoginActivity" />
        <activity android:name="com.git_trailblazer.v1.RepoDetailActivity" />
        <activity android:name="com.git_trailblazer.v1.InitialActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>